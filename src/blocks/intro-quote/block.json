{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "evolutio/intro-quote",
    "version": "0.1.0",
    "title": "Intro Quote",
    "category": "text",
    "icon": "format-quote",
    "description": "A short quote with a link to another page",
    "example": {
        "attributes": {
            "content": "He was the best of us all."
        }
    },
    "attributes": {
        "content": {
            "type": "string",
            "default": ""
        },
        "link": {
            "type": "string",
            "default": "#"
        },
        "linkText": {
            "type": "string",
            "default": "Read more"
        }
    },
    "supports": {
		"background": {
			"backgroundImage": true,
			"backgroundSize": false,
			"__experimentalSkipSerialization": true
		},
        "html": false
    },
    "style": "file:./style-index.css",
    "editorScript": "file:./index.js",
    "render": "file:./render.php"
}
