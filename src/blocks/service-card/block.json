{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "evolutio/service-card",
	"version": "0.1.0",
	"title": "Service Card",
	"category": "text",
	"icon": "format-aside",
	"description": "A card describing one of your services",
	"example": {
		"attributes": {
			"desktopName": "For a company",
			"mobileName": "A company",
			"description": "We offer a wide range of services to help you grow your business"
		}
	},
	"attributes": {
		"desktopName": {
			"type": "string",
			"default": ""
		},
		"mobileName": {
			"type": "string",
			"default": ""
		},
		"description": {
			"type": "string",
			"default": ""
		},
		"url": {
			"type": "string",
			"default": "#"
		}
	},
	"supports": {
		"background": {
			"backgroundImage": true,
			"backgroundSize": true,
			"__experimentalSkipSerialization": true
		},
		"html": false
	},
	"textdomain": "service-card",
	"style": "evolutio-service-card-style",
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}